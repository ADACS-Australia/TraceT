
# Template ONLY - rename this file to template.conf, keep it in the install
# directory, or copy it to /usr/local/etc) and edit it as required.
#
# Do NOT edit this file and commit/push it to the repository with local data,
# ESPECIALLY secure password/s.
#
# WARNING! Python 3's parser for these files now silently reads inline comments
# in as part of the value. Comments must now be on a line by themselves.

# The mail section, defining how emails are sent
[mail]
mailhost = cerberus

# The pyro section, defining how the RPC calls  are configured
[pyro]
ns_host = localhost
ns_port = 9090

# The auth section, defining project IDs and matching secure_key (passwords)
[auth]
C001 = verysecret